extends ../templateChart
block vars
  - var subheader = "Line chart with std. dev. - " + param1 + " vs. " + param2;
  
block script
  script.
    $(function() {
      var div_id = "#lindev_chart_#{chart_id}";
      $(div_id + " .button").on('click', function() {
        $(div_id).remove();
      });
    });